

  <div class="panel panel-default">
  <div class="panel-heading" role="tab">
    <div class="panel-title">
      <a href="#<%= package.object_id %>" data-toggle="collapse" data-parent="#accordion" aria-expanded="false" aria-controls="<%= package.object_id %>">
        <span class="state"><strong>+</strong></span>
        <strong><%= package.name %></strong>
      </a>
    </div>
  </div> <!-- *** end panel-heading *** -->
  <div id="<%= package.object_id %>" class="panel-collapse collapse" role="tabpanel" aria-labelledby="<%= package.object_id %>">
    <div class="panel-body">
      <%= package.description %>
    </div>
  </div> <!-- *** end collapsed item *** -->
</div> <!-- *** end panel *** -->
